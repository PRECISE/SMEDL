object SimpleParserMon
identity
    opaque id;
state
    int currentTime, previousTime, lastDist, currentDist;
events
    imported getTime(int), getLat(float), getLon(float), getDist(float), getSpeed(float);
    internal getSpeed(float);

scenarios
    point_counts: //Input requirements 2,3,4 and Intent 1
        ReadyCounts -> getTime(time) when time > currentTime {previousTime = currentTime; currentTime = time} -> getLat(lat) when lat >= -90 && lat <= 90 -> getLon(lon) when lon >= -180 && lon <= 180 -> ReadyDist 
              else -> Error
        ReadyDist -> getDist(dist) when dist >= 0 {lastDist = currentDist; currentDist = dist; raise getSpeed((currentDist-lastDist)/(currentTime-previousTime))} -> ReadySpeed //intent 2
             else -> Error
   getSpeed:
        ReadySpeed -> getSpeed(speed) when speed <= 6 && speed >= 0 -> ReadyCounts //intent 3
              else -> Error
