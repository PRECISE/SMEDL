object SimpleParserMon
identity
    opaque id;
state
    int currentTime;
events
    imported getTime(int), getLat(float), getLon(float), getDist(float);


scenarios
    point_counts: //Input requirements 2,3,4 and Intent 1
        ReadyCounts -> getTime(time) when time > currentTime {currentTime = time} -> getLat(lat) when lat >= -90 && lat <= 90 -> getLon(lon) when lon >= -180 && lon <= 180 -> getDist(dist) when dist >= 0 -> ReadyCounts
              else -> Error
 
