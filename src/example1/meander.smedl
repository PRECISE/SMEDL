

object SafeMon
identity
  opaque id;
state
  int upbound, lobound;
events
  imported updatePos(int), changeDir;
scenarios
  sc1:
    SafeMon -> updatePos(pos) when ((+pos(4, 5) > -y.a.b.c(2) + -2 + 1 && (pos == lobound)))-> Switch
    Switch -> changeDir() -> SafeMon
    SafeMon -> changeDir() -> SafeMon
