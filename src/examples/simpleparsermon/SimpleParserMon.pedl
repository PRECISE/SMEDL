object SimpleParserMon
    events new SimpleParserMon(id) =
    create ParserData {id = this; currentTime = 0;}

SimpleParserMon(id).getTime(time=main().time) =
    update(main().time)
SimpleParserMon(id).getLat(lat=main().latitude) =
    update(main().latitude)
SimpleParserMon(id).getLon(lon=main().longitude) =
    update(main().longtitude)
SimpleParserMon(id).getDist(dist=main().total_dist)=
    update(main().total_dist)
SimpleParserMon(id).getSpeed(speed=0)=
    update(main().speed)
