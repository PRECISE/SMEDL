object ExplorerStat
state
    int sum,count,targetNum;
events 
    imported retrieved(int);
    internal reachNum() = (sum == targetNum);
    exported output(float);
scenarios
    stat:
        Start -> retrieved(move_count)
         {sum=sum+1;count=count+move_count}->Start
        Start -> reachNum()
         {raise output(count/sum);sum=0;count=0}->Start
