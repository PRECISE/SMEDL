object Explorer_Stat
state
    int sum,sum_count,targetNum;
events 
    imported retrieved(int);
    internal over_threshold() = sum == targetNum;
    exported op(int);
scenarios
    stat:
        Start -> retrieved(move_count){sum = sum + 1; sum_count = sum_count + move_count}-> Start
        Start -> over_threshold() {raise op(sum/sum_count);sum_count = 0} -> Start
