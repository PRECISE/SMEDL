object SPV
    state
        int last_time;
    events
        imported parse_record(int, float, float, int);
    scenarios
        parse_record:
           Start -> parse_record(time,lat,lon,ret) when time > last_time { last_time = time; } -> Start
           Start -> parse_record(time,lat,lon,ret) when time == -1 -> End
           End -> parse_record(time,lat,lon,ret) -> Error